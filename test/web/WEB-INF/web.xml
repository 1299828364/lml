<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <display-name>test</display-name>
    
    <servlet>
        <servlet-name>logIn</servlet-name>
        <servlet-class>servlet.Servlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logIn</servlet-name>
        <url-pattern>/infoma</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>delete</servlet-name>
        <servlet-class>servlet.Servlet3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delete</servlet-name>
        <url-pattern>/www</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>change</servlet-name>
        <servlet-class>servlet.Servlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>change</servlet-name>
        <url-pattern>/sss</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>change1</servlet-name>
        <servlet-class>servlet.ServletTemp1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>change1</servlet-name>
        <url-pattern>/ddd</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>add</servlet-name>
        <servlet-class>servlet.Servlet1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>add</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>enroll</servlet-name>
        <servlet-class>servlet.Servlet4</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>enroll</servlet-name>
        <url-pattern>/enroll</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>jsp/logIn.jsp</welcome-file>
    </welcome-file-list>
</web-app>